<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Patient sequence</field>
            <field name="code">hospital.sequence.patient</field>
            <field name="prefix">PID</field>
            <field name="padding">3</field>
        </record>

    </data>

    <data noupdate="1">

        <record id="seq_hospital_op" model="ir.sequence">
            <field name="name">Patient Op sequence</field>
            <field name="code">hospital.sequence.op</field>
            <field name="prefix">T</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>

    <data noupdate="1">

        <record id="seq_hospital_op_no" model="ir.sequence">
            <field name="name">Patient Op No sequence</field>
            <field name="code">hospital.sequence.op.no</field>
            <field name="prefix">OP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>

</odoo>
